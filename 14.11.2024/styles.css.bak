html {
	font-family: Aral;
	padding: 25px;
}
.container {
	display: flex;
	flex-wrap: wrap;
	align-items: flex-start;
	width: 75%;
	height: 500px;
	border: 1px solid red;
}

.container div {
	border: 1px dotted blue;
	width: 350px;
}
.container .div1 {
	display: flex;
	flex-direction:column;
}
.container .div1 p {
	border: 1px dashed gray;
	padding: 5px;
}	
.container .div2 {
	display: flex;
	flex-direction:row;
	align-items: flex-end;
	justify-content:space-around;
}
.container .div2 p {
	border: 1px dashed gray;
	padding: 10px;
	
}











